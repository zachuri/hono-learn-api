{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
	},
  "include": ["./src/**/*", "bindings.d.ts"]
}
